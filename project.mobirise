ition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container-fluid align-center\">\n        <div mbr-list mbr-list-grow=\"itemsCount\" class=\"blocks-wrapper\">\n        <div class=\"row content-row\">\n            <div class=\"col-lg-6 col-md-6 right-wrapper\">\n                <div class=\"tlv__item-first\"></div>\n                <h1 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">2020</h1>\n                <h3 class=\"mbr-section-subtitle  mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showSubTitle\" data-app-selector=\".mbr-section-subtitle\">MPhil in Civil Engineering</h3>\n                <p class=\"mbr-text mbr-light  mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\"><b>University of Nottingham</b><br>Pursuing PhD under full scholarship by University<br>Expected Graduation Date: 2022</p>\n              </div>\n              \n              <div class=\"col-lg-6 col-md-6 align-center imageContainer\">\n                <div class=\"tlv__pointer\"></div>\n                <div class=\"img-wrapper\">\n                    <img src=\"@PROJECT_PATH@/assets/images/uon-386x386.jpg\" alt=\"Mobirise\" title>\n                </div>\n              </div>\n            </div>\n            <div class=\"row content-row-2\">\n                <div class=\"col-lg-6 col-md-6 imageContainer-2\">\n  